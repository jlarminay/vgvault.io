{
  "scripts": {
    "dev": "start cmd /k vite --port 80 --host",
    "build": "vite build --target=es2020",
    "preview": "npm run build && vite preview --port 5050",
    "test:unit": "vitest --environment jsdom run",
    "test:unit:coverage": "vitest --environment jsdom run --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:e2e:ui": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "update-browsers": "npx browserslist@latest --update-db",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path ./../.gitignore"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "axios": "0.21.1",
    "bootstrap": "^5.1.3",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^1.5.1",
    "vue": "^3.2.31",
    "vue-axios": "^3.4.1",
    "vue-router": "^4.0.12",
    "vue-tabler-icons": "^2.6.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.10",
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "c8": "^7.11.0",
    "cypress": "^9.5.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "miragejs": "^0.1.43",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.8.4",
    "vitest": "^0.5.0"
  }
}
